{
  "name": "fastify-early-hints",
  "version": "0.0.1",
  "description": "Fastify Early Hints",
  "main": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "lint": "npm run lint:standard && npm run lint:typescript",
    "lint:standard": "standard",
    "lint:fix": "standard --fix",
    "lint:typescript": "standard --parser @typescript-eslint/parser --plugin @typescript-eslint/eslint-plugin test/*.ts",
    "unit": "tap test/*.test.js",
    "typescript": "tsd",
    "test": "npm run lint && npm run unit && npm run typescript",
    "coverage": "tap --cov --coverage-report=html test"
  },
  "keywords": [
    "fastify",
    "early",
    "hints"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zekth/fastify-early-hints.git"
  },
  "author": "Vincent Le Goff - @zekth",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zekth/fastify-early-hints/issues"
  },
  "homepage": "https://github.com/zekth/fastify-early-hints#readme",
  "dependencies": {
    "fastify-plugin": "^3.0.0",
    "fastify-warning": "^0.2.0"
  },
  "devDependencies": {
    "@types/node": "^14.14.7",
    "@typescript-eslint/eslint-plugin": "^4.7.0",
    "@typescript-eslint/parser": "^4.7.0",
    "fastify": "^3.8.0",
    "standard": "^16.0.2",
    "tap": "^14.10.8",
    "tsd": "^0.13.1",
    "typescript": "^4.0.5"
  },
  "tsd": {
    "directory": "test"
  }
}
